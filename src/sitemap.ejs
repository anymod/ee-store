<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  <% for(var i = 0; i < entries.length; i++) { %>
    <url>
      <loc><%= entries[i].loc %></loc>
      <lastmod><%= entries[i].lastmod %></lastmod>
      <changefreq><%= entries[i].changefreq %></changefreq>
      <priority><%= entries[i].priority %></priority>
    </url>
  <% } %>

</urlset>
